}
@media screen and (max-width:989px) {
    .outofoffice_post_single_title {
        padding-left: 0px;
        padding-right: 0px;
    }
    .outofoffice_post_single_title h1.article-template__title {
        padding-left: 0px;
        padding-right: 0px;
    }
    .outofoffice_post_single_fimage .media .top_svg {
        top: -10px;
    }
    .outofoffice_post_single_content,
    .outofoffice_post_single_share{
        padding-left: 0px;
        padding-right: 0px;
    }
    .outofoffice_post_single_title .inner,
    .outofoffice_post_single_content .inner,
    .outofoffice_post_single_share .inner{
        max-width: unset;
    }
}
@media screen and (max-width:768px) {
    .outofoffice_post_single_title {
        padding-top: 140px;
    }
    .outofoffice_post_single_title h1.article-template__title {
        font-size: 38px;
    }
    .outofoffice_post_single_fimage {
        overflow: hidden;
    }
    .outofoffice_post_single_mata .caption-with-letter-spacing {
        display: block;
    }
    .outofoffice_post_single_mata .author:before {
        display: none;
    }
    .outofoffice_post_single_fimage .media .top_svg,
    .outofoffice_post_single_fimage .media .bottom_svg{
        display: none;
    }
    .outofoffice_post_single_share {
        padding-bottom: 20px;
    }
    .outofoffice_post_single_mata {
        padding-bottom: 20px;
    }
    .outofoffice_post_single_content h2,
    .outofoffice_post_single_content table tbody tr td h3{
        font-size: 26px;
        padding-top: 15px;
    }
    .outofoffice_post_single_content img {
        padding-top: 15px;
        border: 0px;
        margin-bottom: 16px;
    }
    .outofoffice_post_single_content .inner>div {
        display: block;
        background-color: #fffdfc;
        margin-top: 20px;
        width: 101.5%;
        padding-left: 30px;
        padding-top: 20px;
        padding-right: 30px;
        padding-bottom: 20px;
        border-radius: 28px;
        margin-bottom: 26px;
    }
    .outofoffice_post_single_content ul {
        margin: 0px;
        padding: 0px;
        padding-top: 16px !important;
        padding-right: 0px;
        padding-bottom: 0px;
    }
    .outofoffice_post_single_content ol.s2 li {
        padding-right: 0px;
    }
    .outofoffice_post_single_content h3 {
        font-size: 18px;
        letter-spacing: 0.5px;
        line-height: 1.3;
        padding-bottom: 20px;
    }
    .outofoffice_post_single_content {
        padding-top: 76px;
        padding-bottom: 55px;
    }
}
.focus-none {
    overflow-x: unset !important;
}
