{% schema %}
{
    "name": "Footer",
    "tag": "section",
    "class": "section",
    "settings": [
        {
            "type":"image_picker",
            "id":"image",
            "label":"Footer Logo"
        },
        {
            "type":"text",
            "id":"imageant",
            "label":"Footer Logo ALT"
        },
        {
            "type":"text",
            "id":"instagramuser",
            "default": "#tasteyourrelief",
            "label":"Instagram User Name"
        },
        {
            "type":"text",
            "id":"instagrlink",
            "label":"Instagram Link"
        },
        {
            "type":"text",
            "id":"facebooklink",
            "label":"FaceBook Link"
        },
        {
            "type":"textarea",
            "id":"paymenticon",
            "label":"Footer Payment Icon Svg Code",
            "default": "Payment Icon Svg Code"
        },
        {
            "type":"text",
            "id":"fmamt1",
            "label":"Footer My Account Menu Title",
            "default": "My Account"
        },
        {
	      	"type": "link_list",
	      	"id": "footermenu1",
	      	"default": "main-menu",
	      	"label": "Footer My Account Menu"
	    },
        {
            "type":"text",
            "id":"fmamt2",
            "label":"Footer About Menu Title",
            "default": "About"
        },
	    {
	      	"type": "link_list",
	      	"id": "footermenu2",
	      	"default": "main-menu",
	      	"label": "Footer About Menu"
	    },
        {
            "type":"text",
            "id":"fmamt3",
            "label":"Footer Help Menu Title",
            "default": "Help"
        },
	    {
	      	"type": "link_list",
	      	"id": "footermenu3",
	      	"default": "main-menu",
	      	"label": "Footer Help Menu"
	    },
        {
            "type":"textarea",
            "id":"formbottomtext",
            "label":"Form Bottom Text",
            "default": "*These statements have not been evaluated by the FDA. This product is not intended to diagnose, treat, cure, or prevent any disease."
        }
    ]
}
{% endschema %}