  .the_double_variant_legend input[type=radio]:checked+label.variant-image-button{
      border: solid 2px #023940 !important;
  }
  .the_double_variant_legend .variant-image-button{
    width: 93px;
    text-align: center !important;
    border: solid 1px #eee !important;
    padding: 0 !important;
    border-radius: 10px !important;
    overflow: hidden;
    margin-right: 15px !important;
    background-color: #ffffff !important;
    text-decoration: none !important;
  }
  .the_double_variant_legend .variant-image-button img{
    width: 100%;
    height: auto;
    margin-bottom: -2px;
    line-height: 0;
  }
  .the_double_variant_legend .variant-image-button .tooltip{
    font-size: 11px;
    font-weight: bold;
    text-transform: uppercase;
    line-height: 1;
    padding: 7px;
    display: block;
    color: #4E4A4A;
  }
  .the_double_variant_tooltip{
    position: relative;
    color:#08ACE8;
  }
  .the_double_variant_tooltip svg{
    transform: translateY(3px);
  }
  .the_double_variant_tooltip em{
    position: absolute;
    bottom: 150%;
    left: 50%;
    background-color: #08ACE8;
    padding: 20px;
    border-radius: 20px;
    color: #fff;
    font-style: normal;
    font-size: 12px;
    transform: translateX(-50%);
    width: 265px;
    z-index: 99;
    display: none; 
  }
  .the_double_variant_tooltip em:before{
    content: "";
    width: 12px;
    height: 12px;
    background-color: #08ACE8;
    position: absolute;
    bottom: -6px;
    left: 50%;
    transform: translateX(-50%) rotate(45deg);
  }
  .the_double_variant_tooltip:hover em{
    display: block;
  }
  .the_double_variant_tooltip em ul{
    padding-left:20px;
  }
/*The Double variant image button style - END */


