<style type="text/css">

  {% if section.settings.background_img %}
  .{{ section.settings.sectionclass }} .pro_vacay_expect_bg{
    background-position: bottom right;
    background-repeat: no-repeat;
    background-size: contain;
    position: absolute;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    display: block;
  }
  .{{ section.settings.sectionclass }} .pro_vacay_expect_bg.mobile{
    display: none;
  }
  {% endif %}
  
  {% if section.settings.section_bacground_color != blank %}
    .uid{{ section.id }}{% if section.settings.sectionclass != blank %}.{{ section.settings.sectionclass }}{% endif %} .outf_s4_in{
      background-color:{{section.settings.section_bacground_color}};
    }
  {% endif %}

  .uid{{ section.id }}.nightcap-thc-gummies-ps3 .inner{
    max-width: 1018px;
    margin: auto;
    position: relative;
    padding: 66px 25px 78px 25px;
    width: 90%;
    z-index: 1;
    display: block;
  }
  .uid{{ section.id }}.nightcap-thc-gummies-ps3 .outf_s4_in{}
  .uid{{ section.id }}.nightcap-thc-gummies-ps3 .outf_s4_in .right{
    width: 100%;
    padding-top: 0px;
    padding-bottom: 0px;
    padding-right: 0px;
    display: block;
  }
  .uid{{ section.id }}.nightcap-thc-gummies-ps3 .accordion_body{}
  .uid{{ section.id }}.nightcap-thc-gummies-ps3 .accordion_body .col{
    width: 445px;
  }
  .uid{{ section.id }}.nightcap-thc-gummies-ps3 .accordion_body .accordion_content{}
  .uid{{ section.id }}.nightcap-thc-gummies-ps3 .accordion_body .accordion_content article{}
  .uid{{ section.id }}.nightcap-thc-gummies-ps3 .outf_s4_in .left{ display: none; }
  .nightcap-thc-gummies-ps3 .h5{
    padding-top: 0px;
  }
  .nightcap-thc-gummies-ps3 .h2{
    padding-top: 0px;
  }
  .nightcap-thc-gummies-ps3 .h5,
  .nightcap-thc-gummies-ps3 .h2,
  .nightcap-thc-gummies-ps3  .accordion_body .col .accordion_hand{ color: #EBEFFF !important; }

  .nightcap-thc-gummies-ps3 .accordion_body .col .accordion_hand span svg .st0{ stroke: #EBEFFF !important; }
  .nightcap-thc-gummies-ps3 .accordion_body .col .accordion_hand span svg .st2{ fill: #EBEFFF !important; }
  .nightcap-thc-gummies-ps3 .accordion_body .col .accordion_content article{ background-color: #EBEFFF; color: #023940 !important; }
  .nightcap-thc-gummies-ps3 .outf_s4_in .right_in {
      width: 100%;
  }

  {% if section.settings.text_color != blank %}
    .uid{{ section.id }}{% if section.settings.sectionclass != blank %}.{{ section.settings.sectionclass }}{% endif %} .right_in .h5,
    .uid{{ section.id }}{% if section.settings.sectionclass != blank %}.{{ section.settings.sectionclass }}{% endif %} .right_in .h2,
    .uid{{ section.id }}{% if section.settings.sectionclass != blank %}.{{ section.settings.sectionclass }}{% endif %} .accordion_body .col .accordion_hand{
        color:{{section.settings.text_color}}!important;
    }
    .uid{{ section.id }}{% if section.settings.sectionclass != blank %}.{{ section.settings.sectionclass }}{% endif %} .accordion_body .col .accordion_hand span svg .st0{
        stroke: {{section.settings.text_color}} !important;
    }
    .uid{{ section.id }}{% if section.settings.sectionclass != blank %}.{{ section.settings.sectionclass }}{% endif %} .accordion_body .col .accordion_hand span svg .st2{
        fill: {{section.settings.text_color}} !important;
    }
  {% endif %}

  {% if section.settings.text_bacground_color != blank %}
    .uid{{ section.id }}{% if section.settings.sectionclass != blank %}.{{ section.settings.sectionclass }}{% endif %} .accordion_body .col .accordion_content article{
      background-color:{{section.settings.text_bacground_color}};
    }
  {% endif %}
@media (max-width: 768px) {
    .nightcap-thc-gummies-ps3 .inner {
        padding: 82px 5px 64px 5px !important;
        padding-bottom: 43vw !important;
    }
}
  @media (max-width: 767px){
    {% if section.settings.section_bacground_color_mobile != blank %}
