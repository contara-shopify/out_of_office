	</div>
</section>


{% schema %}
{
    "name": "Home - Subscribe & Save",
    "tag": "section",
    "class": "section",
    "settings": [
        {
          "type": "textarea",
          "id": "subtitle",
          "label": "Sub-Title",
          "default": "Subscribe & Save"
        },
        {
          "type": "textarea",
          "id": "title",
          "label": "Title",
          "default": "Legal THC Delivered to Your Door, <span>Hassle Free</span>"
        },
        {
          "type": "text",
          "id": "buttont",
          "label": "Button Text",
          "default": "Subscribe & Save"
        },
        {
          "type": "textarea",
          "id": "buttonl",
          "label": "Button Link",
          "default": "#"
        },
        {
            "type":"image_picker",
            "id":"image",
            "label":"Image"
        },
        {
          "type": "textarea",
          "id": "imagealt",
          "label": "Image ALT"
        },
        {
            "type": "text",
            "id": "section_class",
            "label": "Section Class"
        }
    ],
    "blocks": [
        {
        "type":"subscribesave",
        "name": "Subscribe & Save",
        "settings": [
            {
                "type":"textarea",
                "label": "Title",
                "id": "title",
                "default": "Save 15% On Every Subscription Order"
            }
        ]
        }
    ],
    "presets": [
        {
          "name": "Home - Subscribe & Save",
          "blocks": []
        }
    ]
}
{% endschema %}